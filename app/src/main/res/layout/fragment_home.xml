<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/feedEntry1TimeTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/time"
        android:textColor="@color/gris_stamina"
        android:textSize="11sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1PaceTitle_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <ImageView
        android:id="@+id/feedEntry1profilePic_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_profile" />

    <TextView
        android:id="@+id/feedEntry1Name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="1dp"
        android:text="Christine Froome"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toTopOf="@+id/feedEntry1profilePic_iv" />

    <TextView
        android:id="@+id/feedEntry1Title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:text="Rodaje matutino"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DateAndLocation_tv" />

    <TextView
        android:id="@+id/feedEntry1DateAndLocation_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Hoy a las 10:28 - MogÃ¡n, Canarias"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Name_tv" />

    <TextView
        android:id="@+id/feedEntry1DistanceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-20dp"
        android:layout_marginTop="10dp"
        android:text="@string/distance"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <TextView
        android:id="@+id/feedEntry1Distance_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-17dp"
        android:layout_marginTop="11dp"
        android:text="3.08 km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DistanceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1Pace_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="11dp"
        android:text="5 min/km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Distance_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DistanceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1Time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="11dp"
        android:text="00:15:23"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Pace_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1TimeTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1PaceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="10dp"
        android:text="@string/pace"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1DistanceTitle_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <ImageView
        android:id="@+id/feedEntry1Map_iv"
        android:layout_width="233dp"
        android:layout_height="155dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="12dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Time_tv"
        app:srcCompat="@drawable/feed_map" />

    <ImageButton
        android:id="@+id/feedEntry1Like_btn"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="9dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_like" />

    <ImageButton
        android:id="@+id/feedEntry1Comment_btn"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Like_btn"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_comment" />

    <ImageButton
        android:id="@+id/feedEntry1Share_btn"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Comment_btn"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_share" />
</androidx.constraintlayout.widget.ConstraintLayout>