        app:layout_constraintTop_toBottomOf="@+id/feedEntry2Title_tv" />

    <ImageView
        android:id="@+id/feedEntry1Map_iv"
        android:layout_width="233dp"
        android:layout_height="155dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="12dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Time_tv"
        app:srcCompat="@drawable/feed_map" />

    <ImageView
        android:id="@+id/feedEntry2Map_iv"
        android:layout_width="233dp"
        android:layout_height="70dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="12dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2TimeTitle_tv"
        app:srcCompat="@drawable/feed_maphalf" />

    <ImageButton
