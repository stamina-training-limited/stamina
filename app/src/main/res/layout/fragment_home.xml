        android:layout_marginTop="9dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_like" />

    <ImageButton
        android:id="@+id/feedEntry1Comment_btn"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Like_btn"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_comment" />

    <ImageButton
        android:id="@+id/feedEntry1Share_btn"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Comment_btn"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Map_iv"
        app:srcCompat="@drawable/ic_share" />
</androidx.constraintlayout.widget.ConstraintLayout>