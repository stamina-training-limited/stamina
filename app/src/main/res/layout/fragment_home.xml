<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/feedEntry1TimeTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="10dp"
        android:text="@string/time"
        android:textColor="@color/gris_stamina"
        android:textSize="11sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1PaceTitle_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <TextView
        android:id="@+id/feedEntry2TimeTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="10dp"
        android:text="@string/time"
        android:textColor="@color/gris_stamina"
        android:textSize="11sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2PaceTitle_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2Title_tv" />

    <ImageButton
        android:id="@+id/home_btn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="575dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="Home button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/map_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        app:srcCompat="@drawable/ic_home" />

    <ImageButton
        android:id="@+id/map_btn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/activity_register_btn"
        app:layout_constraintStart_toEndOf="@+id/home_btn"
        app:srcCompat="@drawable/ic_map" />

    <ImageButton
        android:id="@+id/activity_register_btn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="bottom"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profile_btn"
        app:layout_constraintStart_toEndOf="@+id/map_btn"
        app:srcCompat="@drawable/ic_activity_register" />

    <ImageButton
        android:id="@+id/profile_btn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_register_btn"
        app:srcCompat="@drawable/ic_profile" />

    <ImageView
        android:id="@+id/feedEntry1profilePic_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_profile" />

    <ImageView
        android:id="@+id/feedEntry2ProfilePic_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="320dp"
        app:layout_constraintStart_toStartOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1profilePic_iv"
        app:srcCompat="@drawable/ic_profile" />

    <TextView
        android:id="@+id/feedEntry1Name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="1dp"
        android:text="Christine Froome"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toTopOf="@+id/feedEntry1profilePic_iv" />

    <TextView
        android:id="@+id/feedEntry2Name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="1dp"
        android:text="Ana Pan"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toTopOf="@+id/feedEntry2ProfilePic_iv" />

    <TextView
        android:id="@+id/feedEntry1Title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:text="Rodaje matutino"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DateAndLocation_tv" />

    <TextView
        android:id="@+id/feedEntry2Title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:text="Trote matutino"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2DateAndLocation_tv0" />

    <TextView
        android:id="@+id/feedEntry1DateAndLocation_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Hoy a las 10:28 - MogÃ¡n, Canarias"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Name_tv" />

    <TextView
        android:id="@+id/feedEntry2DateAndLocation_tv0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Hoy a las 09:23 - Vigo, Pontevedra"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2Name_tv" />

    <TextView
        android:id="@+id/feedEntry1DistanceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-20dp"
        android:layout_marginTop="10dp"
        android:text="@string/distance"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <TextView
        android:id="@+id/feedEntry2DistanceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-20dp"
        android:layout_marginTop="10dp"
        android:text="@string/distance"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2Title_tv" />

    <TextView
        android:id="@+id/feedEntry1Distance_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-17dp"
        android:layout_marginTop="11dp"
        android:text="3.08 km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1profilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DistanceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry2Distance_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-17dp"
        android:layout_marginTop="11dp"
        android:text="3.08 km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2ProfilePic_iv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2DistanceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1Pace_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="11dp"
        android:text="5 min/km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Distance_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1DistanceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry2Pace_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="11dp"
        android:text="5 min/km"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2Distance_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2PaceTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1Time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="11dp"
        android:text="00:15:23"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1Pace_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1TimeTitle_tv" />

    <TextView
        android:id="@+id/feedEntry2Time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="11dp"
        android:text="00:15:23"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry2Pace_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry2TimeTitle_tv" />

    <TextView
        android:id="@+id/feedEntry1PaceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="10dp"
        android:text="@string/pace"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/feedEntry1DistanceTitle_tv"
        app:layout_constraintTop_toBottomOf="@+id/feedEntry1Title_tv" />

    <TextView
        android:id="@+id/feedEntry2PaceTitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="10dp"
        android:text="@string/pace"
        android:textColor="@color/gris_stamina"
        android:textSize="10sp"
